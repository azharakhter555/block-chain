    <script>
        $(document).ready(function() {
            // Function to handle MetaMask connection
            function connectMetamask() {
                ethereum.request({ method: 'eth_requestAccounts' })
                    .then(function(accounts) {
                        localStorage.setItem('metamaskId', accounts[0]);
                    
                        performActions();
                    })
                    .catch(function(error) {
                        console.error("Error connecting to MetaMask:", error);
                    });
            }

            // Function to perform actions based on login status
            function performActions() {
                const metamaskId = localStorage.getItem('metamaskId');
                if (metamaskId) {
                    window.location.href = '/users';
                    // Perform actions here...
                } else {
                    console.log("Not logged in");
                    // Perform actions here...
                }
            }

            // Check for MetaMask presence and handle login button click
            if (window.ethereum) {
                $("#loginBtn").click(function() {
                    connectMetamask();
                });
            } else {
                console.log("MetaMask not detected.");
                // Display message to the user to configure MetaMask
                $("#metamaskMessage").text("Please configure MetaMask to use this feature.");
                $("#loginBtn").prop("disabled", true);
            }

         
        });
    </script>

